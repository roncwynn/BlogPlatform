@model IEnumerable<BlogPlatform.Models.Genre>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1>Index Page</h1>
    @foreach (var genre in Model)
    {
        <h3><a href="/Genre/Details/@genre.Id">Genre Name: @genre.Name</a></h3>
    }
</body>
</html>
